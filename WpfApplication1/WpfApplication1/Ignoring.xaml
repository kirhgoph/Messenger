<Window x:Class="InstantMessenger.Ignoring"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Seeing" Height="295.806" Width="300">
    <Grid>
        <ListBox x:Name="lbx_IgnoringList" HorizontalAlignment="Left" Height="107" Margin="10,45,0,0" VerticalAlignment="Top" Width="272"/>
        <Label Content="List of Ignoring" HorizontalAlignment="Left" Height="30" Margin="10,10,0,0" VerticalAlignment="Top" Width="85"/>
        <Button Content="Close" HorizontalAlignment="Left" Height="26" Margin="225,228,0,0" VerticalAlignment="Top" Width="57" IsDefault="True" IsCancel="True" Click="Button_Click"/>
        <ComboBox x:Name="cbx_DeleteIgnoring" HorizontalAlignment="Left" Margin="10,157,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cbx_DeleteIgnoring_SelectionChanged"/>
        <ComboBox x:Name="cbx_AddIgnoring" HorizontalAlignment="Left" Margin="10,189,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cbx_AddIgnoring_SelectionChanged"/>
        <Button x:Name="btn_DeleteIgnoring" Content="Delete Ignoring" HorizontalAlignment="Left" Margin="159,157,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="btn_DeleteIgnoring_Click"/>
        <Button x:Name="btn_AddIgnoring" Content="Add Ignoring" HorizontalAlignment="Left" Margin="159,189,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="btn_AddIgnoring_Click"/>
    </Grid>
</Window>
