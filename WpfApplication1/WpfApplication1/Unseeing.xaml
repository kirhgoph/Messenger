<Window x:Class="InstantMessenger.Unseeing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Seeing" Height="292.821" Width="300">
    <Grid>
        <ListBox x:Name="lbx_UnseeingList" HorizontalAlignment="Left" Height="107" Margin="10,45,0,0" VerticalAlignment="Top" Width="272"/>
        <Label Content="List of Unseeing" HorizontalAlignment="Left" Height="30" Margin="10,10,0,0" VerticalAlignment="Top" Width="85"/>
        <Button Content="Close" HorizontalAlignment="Left" Height="26" Margin="225,228,0,0" VerticalAlignment="Top" Width="57" IsDefault="True" IsCancel="True" Click="Button_Click"/>
        <ComboBox x:Name="cbx_DeleteUnseeing" HorizontalAlignment="Left" Margin="10,157,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cbx_DeleteUnseeing_SelectionChanged"/>
        <ComboBox x:Name="cbx_AddUnseeing" HorizontalAlignment="Left" Margin="10,189,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cbx_AddUnseeing_SelectionChanged"/>
        <Button x:Name="btn_DeleteUnseeing" Content="Delete Unseeing" HorizontalAlignment="Left" Margin="159,157,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="btn_DeleteUnseeing_Click"/>
        <Button x:Name="btn_AddUnseeing" Content="Add Unseeing" HorizontalAlignment="Left" Margin="159,189,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="btn_AddUnseeing_Click"/>
    </Grid>
</Window>

